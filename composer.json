{
    "name": "kiwi-suite/application",
    "description": "kiwi-suite/application",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Thomas Payer"
        }
    ],
    "require": {
        "php": "^7.1",
        "kiwi-suite/servicemanager": "^0.0.2",
        "http-interop/http-middleware": "0.4.1",
        "zendframework/zend-expressive": "^2.0",
        "zendframework/zend-expressive-fastroute": "^2.1",
        "zendframework/zend-stdlib": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.6",
        "friendsofphp/php-cs-fixer": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "KiwiSuite\\Application\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KiwiSuiteMisc\\Application\\": "tests/misc/",
            "KiwiSuiteTest\\Application\\": "tests/src/"
        }
